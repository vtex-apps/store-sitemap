{
  "memory": 512,
  "ttl": 60,
  "timeout": 55,
  "minReplicas": 2,
  "maxReplicas": 10,
  "workers": 4,
  "routes": {
    "sitemap": {
      "path": "/sitemap.xml",
      "public": true
    },
    "sitemapEntry": {
      "path": "/sitemap/:path",
      "public": true
    },
    "generateSitemap": {
      "path": "/generate-sitemap",
      "public": false
    },
    "robots": {
      "path": "/robots.txt",
      "public": true
    }
  }
}
